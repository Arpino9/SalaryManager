<?xml version="1.0" encoding="utf-8" ?>
<appSettings>
	<!-- 和暦フォーマット -->
	<add key="YearFormat" value="ggy"/>
	
	<!-- CSVの保存ディレクトリ -->
	<add key="DirectoryCSV" value="C:\Users\OKAJIMA\Downloads"/>
	
	<!-- 給与明細Excelのテンプレートファイル名 -->
	<add key="ExcelTemplateName" value="Template_Payslip"/>

	<!-- オプションの保存形式(JSON/XML) -->
	<add key="SavingExtension" value="JSON"/>

	<!-- XMLのファイル名 -->
	<add key="XMLName" value="SalaryManager"/>

	<!-- JSONの設定ファイル名 -->
	<add key="JSONName" value="Settings"/>

	<!-- フォントファミリ -->
	<add key="FontFamily" value="Yu Gothic UI"/>

	<!-- フォントサイズ-->
	<add key="FontSize" value="12"/>

	<!-- 初期表示時にデフォルト明細を表示する-->
	<add key="ShowDefaultPayslip" value="false"/>
	
	<!-- システム名-->
	<add key="SystemName" value="明細管理システム(仮)"/>
</appSettings>